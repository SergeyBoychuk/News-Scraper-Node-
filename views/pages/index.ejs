<html lang="en">
<head>
	<% include ../partials/head %>
</head>
<body class="container">

	<header>
		<% include ../partials/header %>
	</header>

		<div>
			<h1>This is great</h1>
			<p>Welcome to templating using EJS</p>
			
				<ul class=" list-unstyled content-wrapper">
					<% dataDisplayed.forEach(function(title, index) { %>
						<% for(var i = 0; i < title.length; i++) { %> 
							<div class="news-box">
								<li class="headline"><%= dataDisplayed[0][i] %></li>
								<li><%= dataDisplayed[1][i] %></li>
								<img class="img-responsive" src="<%= dataDisplayed[2][i] %>">
							</div>
						<% } %> 
					<% }); %> 
				</ul>
			
		</div>

	<footer>
		<% include ../partials/footer %>
	</footer>

</body>

</html>

